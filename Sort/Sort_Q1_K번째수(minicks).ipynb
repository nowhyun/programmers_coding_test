        "def solution(array, commands):\n",
        "    answer = []\n",
        "    #command 별로 사용\n",
        "    for com in commands:\n",
        "        list = sorted(array[com[0]-1:com[1]])\n",
        "        answer.append(list[com[2]-1])\n",
        "    return answer"
      ],
